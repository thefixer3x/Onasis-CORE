# Onasis-CORE Environment Configuration Template
# Copy this file to .env and fill in the values

# Router Configuration
ROUTER_PORT=3000

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_KEY=your-supabase-service-key

# SD-Ghost Protocol Configuration
SD_GHOST_VPS_URL=http://your-sd-ghost-vps-url:3000
SD_GHOST_SUPABASE_URL=https://your-sd-ghost.supabase.co

# Onasis-CORE Partnership Management
ONASIS_SUPABASE_URL=https://your-onasis-project.supabase.co
ONASIS_SUPABASE_SERVICE_KEY=your-onasis-service-key

# JWT Configuration
JWT_SECRET=your-jwt-secret-min-32-chars

# Control Room Configuration
CONTROL_ROOM_PORT=4000