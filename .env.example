# Onasis-CORE Environment Configuration Template
# Copy this file to .env and fill in the values

# Router Configuration
ROUTER_PORT=3000

# Supabase Configuration
SUPABASE_URL=https://<project-ref>.supabase.co
SUPABASE_ANON_KEY=REDACTED_SUPABASE_ANON_KEY
SUPABASE_SERVICE_KEY=REDACTED_SUPABASE_SERVICE_ROLE_KEY

# SD-Ghost Protocol Configuration
SD_GHOST_VPS_URL=http://your-sd-ghost-vps-url:3000
SD_GHOST_SUPABASE_URL=https://<project-ref>.supabase.co

# Onasis-CORE Partnership Management
ONASIS_SUPABASE_URL=https://<project-ref>.supabase.co
ONASIS_SUPABASE_SERVICE_KEY=REDACTED_SUPABASE_SERVICE_ROLE_KEY

# JWT Configuration
JWT_SECRET=REDACTED_JWT_SECRET

# Control Room Configuration
CONTROL_ROOM_PORT=4000