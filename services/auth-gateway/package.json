{
  "name": "auth-gateway",
  "version": "0.2.0",
  "description": "Supabase-integrated authentication gateway for Lan-Onasis platform",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p .",
    "start": "node start.js",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "test:unit": "vitest run tests/unit --reporter=verbose",
    "test:integration": "vitest run tests/integration --reporter=verbose",
    "test:security": "vitest run tests/security --reporter=verbose --passWithNoTests",
    "test:load": "k6 run tests/load/oauth-flow.js",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "outbox:forward": "tsx src/workers/outbox-forwarder.ts",
    "bootstrap:supabase": "tsx scripts/bootstrap-from-supabase.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.90.1",
    "bun": "^1.3.6",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.3",
    "ms": "^2.1.3",
    "node-fetch": "^3.3.2",
    "pg": "^8.17.1",
    "prom-client": "^15.1.3",
    "qs": "^6.14.1",
    "winston": "^3.19.0",
    "ws": "^8.19.0",
    "xss": "^1.0.15",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.9",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.17",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "k6": "^0.0.0",
    "prettier": "^3.8.0",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "overrides": {
    "body-parser": "^2.2.1",
    "js-yaml": "^4.1.1",
    "qs": "^6.14.1",
    "diff": "^8.0.3"
  }
}
